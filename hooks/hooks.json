{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "alice hook session-start",
            "timeout": 5,
            "env": {
              "JWZ_STORE": "~/.claude/alice/.jwz",
              "TISSUE_STORE": "~/.claude/alice/.tissue"
            }
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "alice hook user-prompt",
            "timeout": 5,
            "env": {
              "JWZ_STORE": "~/.claude/alice/.jwz",
              "TISSUE_STORE": "~/.claude/alice/.tissue"
            }
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "alice hook stop",
            "timeout": 30,
            "env": {
              "JWZ_STORE": "~/.claude/alice/.jwz",
              "TISSUE_STORE": "~/.claude/alice/.tissue"
            }
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "alice hook post-tool-use",
            "timeout": 5,
            "env": {
              "JWZ_STORE": "~/.claude/alice/.jwz",
              "TISSUE_STORE": "~/.claude/alice/.tissue"
            }
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "alice hook session-end",
            "timeout": 5,
            "env": {
              "JWZ_STORE": "~/.claude/alice/.jwz",
              "TISSUE_STORE": "~/.claude/alice/.tissue"
            }
          }
        ]
      }
    ]
  }
}
